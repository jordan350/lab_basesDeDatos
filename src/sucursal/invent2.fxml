<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="sucursal.InventarioController">
   <children>
      <VBox layoutX="14.0" layoutY="14.0" prefHeight="373.0" prefWidth="572.0">
         <children>
            <HBox alignment="BASELINE_CENTER" prefHeight="51.0" prefWidth="572.0">
               <children>
                  <Label text="INVENTARIO">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="34.0" prefWidth="572.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="91.0" text="Nombre: " />
                  <TextField fx:id="nombre" prefHeight="25.0" prefWidth="157.0" />
               </children>
            </HBox>
            <HBox prefHeight="36.0" prefWidth="572.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="91.0" text="Id: " />
                  <TextField fx:id="id" prefHeight="25.0" prefWidth="156.0" />
               </children>
            </HBox>
            <HBox prefHeight="40.0" prefWidth="572.0">
               <children>
                  <Label prefHeight="17.0" prefWidth="90.0" text="Precio: " />
                  <TextField fx:id="precio" prefHeight="25.0" prefWidth="159.0" />
               </children>
            </HBox>
            <TableView fx:id="table" prefHeight="151.0" prefWidth="572.0">
              <columns>
                <TableColumn prefWidth="75.0" text="ID" />
                <TableColumn prefWidth="75.0" text="Precio" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox alignment="BOTTOM_CENTER" prefHeight="56.0" prefWidth="572.0" spacing="50.0">
               <children>
                  <Button fx:id="guardar" mnemonicParsing="false" onAction="#Guardar_Producto" text="Guardar" />
                  <Button fx:id="mostrar" mnemonicParsing="false" onAction="#Mostrar_Productos" text="Mostrar" />
                  <Button fx:id="salir" mnemonicParsing="false" onAction="#Volver" text="Salir" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
